    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "internal-ip": "^4.0.0",
    "logger": "0.0.1",
    "material-ui": "^0.20.2",
    "mongoose": "^5.4.13",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-ace": "^6.4.0",
    "react-dom": "^16.5.2",
